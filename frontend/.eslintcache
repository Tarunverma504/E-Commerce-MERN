ssName=\"btn btn-primary btn-block\" onClick={checkoutHandler}>Check out</button>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </Fragment>\r\n            )}\r\n        </Fragment>\r\n    )\r\n}\r\n\r\nexport default Cart","E:\\Projects\\E-Commerce-U\\frontend\\src\\components\\cart\\Shipping.js",[],"E:\\Projects\\E-Commerce-U\\frontend\\src\\components\\cart\\CheckoutSteps.js",[],"E:\\Projects\\E-Commerce-U\\frontend\\src\\components\\cart\\ConfirmOrder.js",[],"E:\\Projects\\E-Commerce-U\\frontend\\src\\components\\cart\\Payment.js",[],"E:\\Projects\\E-Commerce-U\\frontend\\src\\reducers\\orderReducers.js",[],"E:\\Projects\\E-Commerce-U\\frontend\\src\\constants\\orderConstants.js",[],"E:\\Projects\\E-Commerce-U\\frontend\\src\\actions\\orderActions.js",["432"],"import axios from 'axios';\r\n\r\nimport {\r\n    CREATE_ORDER_REQUEST,\r\n    CREATE_ORDER_SUCCESS,\r\n    CREATE_ORDER_FAIL,\r\n    MY_ORDERS_REQUEST,\r\n    MY_ORDERS_SUCCESS,\r\n    MY_ORDERS_FAIL,\r\n    ALL_ORDERS_REQUEST,\r\n    ALL_ORDERS_SUCCESS,\r\n    ALL_ORDERS_FAIL,\r\n    UPDATE_ORDER_SUCCESS,\r\n    UPDATE_ORDER_REQUEST,\r\n    UPDATE_ORDER_FAIL,\r\n    DELETE_ORDER_REQUEST,\r\n    DELETE_ORDER_SUCCESS,\r\n    DELETE_ORDER_FAIL,\r\n    ORDER_DETAILS_REQUEST,\r\n    ORDER_DETAILS_SUCCESS,\r\n    ORDER_DETAILS_FAIL,\r\n    CLEAR_ERRORS\r\n} from \"../constants/orderConstants\";\r\n\r\nexport const createOrder = (order) => async(dispatch, getState) =>{\r\n    try{\r\n        dispatch({\r\n            type: CREATE_ORDER_REQUEST\r\n        })\r\n        const Token= localStorage.getItem(\"Token\");\r\n\r\n        const config = {\r\n            headers:{\r\n                'Content-Type': 'application/json',\r\n                'authorization':`Bearer ${Token}`\r\n            }\r\n        }\r\n\r\n        const { data } = await axios.post(`${process.env.REACT_APP_PORT}/api/v1/order/new`, order, config)\r\n        dispatch({\